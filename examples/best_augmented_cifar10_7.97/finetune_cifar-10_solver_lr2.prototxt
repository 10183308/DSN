device_id: 2
train_net: "finetune_cifar-10.prototxt"
test_net: "finetune_cifar-10_val.prototxt"
test_iter: 100
test_interval: 500
base_lr: 0.0001
lr_policy: "step"
gamma: 0.05
stepsize: 100001

display: 500
max_iter: 30001
momentum: 0.9
weight_decay: 0.0005

snapshot: 1000
snapshot_prefix: "finetune_lr2_nosvm_cifar-10"
solver_mode: 1
info {
  type: "weight"
  interval: 100
}
info {
  type: "mean_train_loss"
  interval: 100
}
info {
  type: "layer_loss"
  interval: 500
}
